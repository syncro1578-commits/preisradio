{
  "framework": "nextjs",
  "regions": ["fra1"],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://api.preisradio.de",
    "NEXT_PUBLIC_API_BASE": "/api",
    "NEXT_PUBLIC_SITE_NAME": "PrixRadio"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.preisradio.de/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}